using System;
using System.Collections.Generic;
using System.Linq;
			
public class Dostov
{
  public int srok;
  public int raz;
  public int reasons;
  public int freq()
  {
    if (raz/srok > 2) return 100;
    //for ( ; raz/srok > reasons; )
    else if (1 < raz/srok && raz/srok <= 2) return 50;
      else if (0.6 < raz/srok && raz/srok <= 1) return 25;
         else return 0;
  }
}

class Program 
{
  static void Main (string [] args)
  {
    Dostov bzik = new Dostov();
    bzik.srok = 12;
    bzik.raz = 24;
    bzik.reasons = 70;
    var res = (bzik.freq() + bzik.reasons)/2;
    Console.WriteLine(res);
    //string res = metod1();
    
     
    /*Chtoto stul = new Chtoto();
    stul.Nazvanie = "stul";
    stul.Prodaetsa = true;
    stul.Price = 1500;
    stul.Info();*/
  }
}
